{
  "name": "@braid/vue-formulate-i18n",
  "version": "1.15.0",
  "description": "Internationalization (i18n) support for vue-formulate",
  "main": "dist/locales.umd.js",
  "module": "dist/locales.esm.js",
  "unpkg": "dist/locales.min.js",
  "scripts": {
    "build": "npm run build:esm & npm run build:umd & npm run build:iife & npm run build:unpkg & wait && echo \"Build complete:\nesm: $(gzip -c dist/locales.esm.js | wc -c)b gzip\numd: $(gzip -c dist/locales.umd.js | wc -c)b gzip\nmin: $(gzip -c dist/locales.min.js | wc -c)b gzip\"",
    "build:esm": "rollup --config build/rollup.config.js --format es --file dist/locales.esm.js",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/locales.umd.js",
    "build:iife": "rollup --config build/rollup.config.js --format iife --file dist/locales.min.js",
    "build:unpkg": "./build/locales.sh",
    "test": "NODE_ENV=test jest --config test/jest.conf.js",
    "test:watch": "NODE_ENV=test jest --config test/jest.conf.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/wearebraid/vue-formulate-i18n.git"
  },
  "keywords": [
    "Vue",
    "i18n",
    "internationalization",
    "vue-formulate"
  ],
  "author": "Justin Schroeder <justin@wearebraid.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@vue/component-compiler-utils": "^3.2.0",
    "@vue/test-utils": "^1.0.5",
    "autoprefixer": "^9.8.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.5.1",
    "cssnano": "^4.1.10",
    "cypress": "^4.12.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "flush-promises": "^1.0.2",
    "jest": "^25.5.4",
    "jest-vue-preprocessor": "^1.7.1",
    "node-sass": "^7.0.0",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.2",
    "rollup": "^1.31.1",
    "rollup-plugin-internal": "^1.0.4",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-terser": "^5.3.1",
    "typescript": "^3.9.7",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.6",
    "vue-runtime-helpers": "^1.1.2",
    "vue-template-compiler": "^2.6.12",
    "vue-template-es2015-compiler": "^1.9.1",
    "watch": "^1.0.2"
  }
}
